<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.paint.*?>
<VBox spacing="12" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
      fx:controller="cs151.application.StudentListController" prefWidth="800" prefHeight="500">
    <Label text="Student Profiles" style="-fx-font-size:18;" />
    <TableView editable="false" fx:id="StudentsList">
        <columns>
            <TableColumn prefWidth="100" text="Name" fx:id="NameColumn"/>
            <TableColumn prefWidth="100" text="Employment" fx:id="EmploymentColumn"/>
            <TableColumn prefWidth="100" text="Workplace" fx:id="WorkplaceColumn"/>
            <TableColumn prefWidth="100" text="Year" fx:id="YearColumn"/>
            <TableColumn prefWidth="100" text="Role" fx:id="RoleColumn"/>
            <TableColumn prefWidth="200" text="Databases" fx:id="DatabasesColumn"/>
            <TableColumn prefWidth="200" text="Languages" fx:id="LanguagesColumn"/>
            <TableColumn prefWidth="100" text="WhiteList" fx:id="WhiteListColumn"/>
            <TableColumn prefWidth="100" text="BlackList" fx:id="BlackListColumn"/>
            <TableColumn prefWidth="400" text="Comments" fx:id="CommentsColumn"/>
        </columns>
        
    </TableView>

    <HBox spacing="10">
        <Button fx:id="backBtn" text="Back"/>
    </HBox>
</VBox>
